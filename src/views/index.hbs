<style>
    .img-container {
        position: relative;
        overflow: hidden;
    }

    .img-cover {
        width: 100%;
        height: auto;
        transition: transform 0.5s ease;
    }

    .img-overlay {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        text-align: center;
        color: white;
        opacity: 1;
        /* Cambiado a 1 para que el texto sea visible desde el principio */
        transition: opacity 0.5s ease;
    }

    .img-container:hover .img-cover {
        transform: scale(1.1);
    }

    .img-container:hover .img-overlay {
        opacity: 1;
    }

